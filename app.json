{
  "expo": {
    "name": "Credit Swag",
    "slug": "creditswag",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Credit Swag is a budgeting app for students."
  },
  "infoPlist": {
    "NSAppTransportSecurity": {
      "NSExceptionDomains": {
        "localhost:5000": true,
        "http://localhost:5000": true,
        "http://127.0.0.1:5000/": true
      }
    }
  }
}